generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model User {
  id Int @id @default(autoincrement())
  email String @unique
  password String
  createdAt DateTime @default(now())

  jokeLogs JokeLog[]
}

model JokeLog {
  id Int @id @default(autoincrement())
  userId Int
  joke String
  moodAfter String
  createdAt DateTime @default(now())

  user User @relation(fields: [userId], references: [id])
}